{
  "name": "pagal-player",
  "version": "0.1.0",
  "description": "A new media player for the world.",
  "main": "app://pagal/index.html",
  "window": {
    "title": "Pagal media player",
    "show": true,
    "toolbar": false,
    "frame": true,
    "position": "center",
    "width": 1080,
    "height": 640,
    "icon": "lib/img/icon.png",
    "min_width": 400,
    "min_height": 50
  },
  "wcjs-prebuilt": {
    "runtime": "nw",
    "runtimeVersion": "v0.12.3",
    "version": "v0.2.3",
    "arch": "ia32"
  },
  "single-instance": true,
  "dependencies": {
    "drag-and-drop-files": "0.0.1",
    "findit": "2.0.0",
    "hotkeys": "1.0.0",
    "jquery-ui": "github:jaruba/jquery-ui",
    "movie-art": "1.0.6",
    "needle": "1.1.2",
    "toastr": "2.1.3",
    "ultimate-parser": "0.0.3",
    "video-name-parser": "^1.4.0",
    "wcjs-player": "github:hlgkb/wcjs-player",
    "workerjs": "^0.1.1",
    "xml2js": "^0.4.17",
    "xmlbuilder": "^4.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hlgkb/pagal-player.git"
  },
  "chromium-args": "--d3d9 --disable-d3d11 --gpu-no-context-lost --ignore-gpu-blacklist",
  "author": "pagal <bibash1270@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.0",
    "gulp-install": "^0.6.0",
    "gulp-jshint": "^2.0.1",
    "gulp-jslint": "^1.0.5",
    "gulp-jslint-simple": "^1.0.2",
    "gulp-minify": "0.0.14",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "innosetup-compiler": "^5.5.9",
    "jshint": "^2.9.3",
    "jshint-stylish": "^2.2.1",
    "nw-builder": "^3.1.2"
  },
  "nwjsBuilder": {
    "productName": "pagalPlayer",
    "productVersion": "0.1.0 Beta",
    "fileDescription": "pagalPlayer.exe",
    "copyright": "Copyright (C) Pagal INC",
    "fileVersion": "0.12.3",
    "companyName": "Pagal INC",
    "originalFilename": "pagal.exe"
  }
}
